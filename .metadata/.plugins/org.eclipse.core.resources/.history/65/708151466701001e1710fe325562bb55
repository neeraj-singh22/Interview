package Pattern;

public class starPattern4 {   

	public static void main(String[] ar) {
		
		 for(int i = 5; i >= 1; i --){

		     

             for(int k = i-1 ; k>=1;k--){

                  System.out.print('.');

             }

             for(int j = i ; j <= 5; j++){

                   System. out.print(j);

             }

               System.out.println();

             }

       }

}
//5
//45 
//345 
//2345
//12345